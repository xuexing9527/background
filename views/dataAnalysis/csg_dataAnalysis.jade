extends ../partial/dataAnalysis_left
block append right
    .right
        #anlysisrighttop
            .anlysisBreadcrumb_navigation
                a(href="http://localhost:9298/dataAnalysis/rendering/dataAnalysis") 数据分析
            .anlysistop
                form.anlysistopform
                    // 关键字
                    input.textones(type='text', placeholder='关键字', id="An_keywords", name="An_keywords" )
                    // 主题
                    select.theme(id='An_theme', name='An_theme')
                        option(value='行业类型') 行业类型
                        option(value='汽车') 汽车
                    .layui-input-inline.select_width
                        input#LAY_demorange_s.layui-input.anlysistime(placeholder='开始日期' name="starttime")
                    .layui-input-inline.select_width
                        input#LAY_demorange_e.layui-input.anlysisendtime(placeholder='截止日期')
                    input.btnones(type='button', value='查询' id="An_search")
            .query_content_content
                .query_content_content_header.clearfix
                    p
                        a(href="javascript:void(0)" id="An_dataserching") 数据查询
                    p
                        a(href="javascript:void(0)" id="An_searchingrel") 查询结果
                .query_content_content_box_part01
                    .query_content_content_content
                        table#an_table(border='1', cellpadding='0', cellspacing='0')
                            thead
                                tr
                                    th.query_th01 选择
                                    th.query_th02 任务名称
                                    th.query_th07 名称/表名
                                    th.query_th03 包含数量
                                    th.query_th04 生成时间
                                    th.query_th05 行业
                                    th.query_th06 操作
                            tbody#checkAll
                                tr
                                    td 暂无数据
                    .query_content_content_footer.clearfix
                        .query_footer_left
                            input(type="button",value="全选").query_footer_left_choise#allcheck
                            a.query_footer_left_save(href='javascript:void(0)' id="an_save") 保存
                        .query_footer_right
                            .pager.pager01
                    .query_content_choise_button
                        a(href='javascript:void(0)' id="carryUp_an") 分析选中的内容
                    .query_popup
                        p.relpppp#an_search_a
                                //a(href="javascript:void(0)" id="an_guanxi_liji") 查看详情
                        #relCanvas 暂无数据
                .query_content_content_box_part02
                    .jxy_query_content_xbox
                        .jxy_query_content_leftbox
